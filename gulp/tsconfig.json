{
  "compilerOptions": {
    "alwaysStrict": true,
    "baseUrl": ".",
    "importHelpers": false,
    "lib": ["ES2015"],
    "module": "ESNext",
    "moduleResolution": "Node",
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "pretty": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true
  }
}
